{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [".docusaurus", "build"]
}
